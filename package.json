{
  "name": "@iamnnort/telegraf-helper",
  "version": "1.2.12",
  "description": "Helpers for Telegraf.js framework - Simple - Customizable - Convenient",
  "keywords": [
    "telegraf",
    "telegram",
    "bot"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:message": "yarn ts-node ./examples/message",
    "start:telegram": "yarn ts-node ./examples/telegram",
    "start:telegraf": "yarn ts-node ./examples/telegraf",
    "build": "tsup",
    "lint": "eslint ./{src,examples}/**/*.ts --fix",
    "unit": "jest",
    "test": "yarn lint && yarn unit"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamnnort/telegraf-helper.git"
  },
  "author": "Nort Red",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamnnort/telegraf-helper/issues"
  },
  "homepage": "https://github.com/iamnnort/telegraf-helper#readme",
  "dependencies": {
    "telegraf": "^4.14.0",
    "typegram": "^5.2.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
